.u-wrp-bnr {
  width: 100%;
  height: 300rpx;
  display: block;
  bottom: 30rpx;
  position: relative;
  background: #f0f0f0;
}

.many-text {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  padding-left: 20px;
  padding-right: 10px;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 10;
}

.u-img-slide {
  width: 100%;
  height: inherit;
}

.video-list {
  width: 300rpx;
}

.border {
  border-radius: 2%;
  background: white;
  margin: 30rpx;
  padding: 10rpx;
  border-style: solid;
  box-shadow: 0px 2px 4px 2px #ddd;
  border-width: 0rpx;
}
